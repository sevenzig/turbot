# Environment Variables Template
# Copy this file to .env.local and customize values for your project

# Application Configuration
VITE_APP_NAME=[PROJECT_NAME]
VITE_APP_VERSION=1.0.0
VITE_APP_URL=https://yourdomain.com

# API Configuration
VITE_API_URL=https://api.yourdomain.com
VITE_API_TIMEOUT=10000

# Contact Form
VITE_CONTACT_FORM_ENDPOINT=https://formspree.io/f/your-form-id
VITE_CONTACT_EMAIL=[BUSINESS_EMAIL]

# Analytics (Optional)
VITE_GA_TRACKING_ID=G-XXXXXXXXXX
VITE_GTM_ID=GTM-XXXXXXX

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_PWA=false
VITE_ENABLE_DARK_MODE=false

# Development Settings
VITE_ENABLE_DEBUG=false
VITE_ENABLE_MOCK_API=false

# Social Media (Optional)
VITE_FACEBOOK_URL=[SOCIAL_FACEBOOK]
VITE_TWITTER_URL=[SOCIAL_TWITTER]
VITE_INSTAGRAM_URL=[SOCIAL_INSTAGRAM]
VITE_LINKEDIN_URL=[SOCIAL_LINKEDIN]

# Business Information
VITE_BUSINESS_NAME=[BUSINESS_NAME]
VITE_BUSINESS_PHONE=[BUSINESS_PHONE]
VITE_BUSINESS_EMAIL=[BUSINESS_EMAIL]
VITE_BUSINESS_ADDRESS=[BUSINESS_ADDRESS]

# Map Integration (Optional)
VITE_GOOGLE_MAPS_API_KEY=your-google-maps-api-key
VITE_MAP_LATITUDE=[BUSINESS_LATITUDE]
VITE_MAP_LONGITUDE=[BUSINESS_LONGITUDE] 